{
  "name": "bilza",
  "version": "0.0.11",
  "description": "Bilza is a javascript framework for creating business and educational visualizations in the browser using HTML5 canvas",
  "main": "src/index.js",
  "scripts": {
    "rollup:help": "rollup --help",
    "rollup:es": "rollup ./src/index.js --file ./build/bilza.js --format es",
    "rollup:cjs": "rollup ./src/index.js --file ./build/bilza_cjs.js --format cjs",
    "test": "jest --watchAll --detectOpenHandles"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/skillzaa/bilza"
  },
  "keywords": [],
