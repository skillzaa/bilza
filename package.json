  "license": "MIT",
  "bugs": {
    "url": "https://github.com/skillzaa/bilza/issues"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.17.7",
    "@types/jest": "^27.4.1",
    "canvas": "^2.9.1",
    "jest": "^27.5.1",
    "jest-canvas-mock": "^2.3.1",
    "rollup": "^2.70.1",
    "typescript": "^4.5.5"
  }
}
