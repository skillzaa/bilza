{
  "name": "bilza",
  "version": "0.0.26",
  "description": "Bilza is a javascript framework/library specially designed for creating business and educational Visualizations / Presentations / Slides using HTML5 canvas.",
  "main": "src/bilza.js",
  "scripts": {
    "rollup": "rollup -c",
    "rollup:help": "rollup --help",
    "rollup:es": "rollup ./src/bilza.js --file ./build/bilza.js --format es",
    "rollup:cjs": "rollup ./src/index.js --file ./build/bilza_cjs.js --format cjs",
    "test": "jest --watchAll --detectOpenHandles"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/skillzaa/bilza"
  },
  "keywords": [
    "animation",
    "javascipt animation",
    "canvas",
    "html5 canvas",
    "business presentation",
    "slides"
  ],
  "author": "Bilal Tariq",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/skillzaa/bilza/issues"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.17.7",
    "@types/jest": "^27.4.1",
    "canvas": "^2.9.1",
    "jest": "^27.5.1",
    "jest-canvas-mock": "^2.3.1",
    "rollup": "^2.70.1",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.5.5",
    "vitest": "^0.15.2"
  }
}
